{
	"cli": {
		"version": ">= 12.6.0",
		"appVersionSource": "remote"
	},
	"build": {
		"simulator": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "development"
			},
			"ios": {
				"simulator": true
			}
		},
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "preview"
			}
		},
		"production": {
			"autoIncrement": true,
			"env": {
				"APP_VARIANT": "production"
			}
		}
	},
	"submit": {
		"production": {}
	}
}
